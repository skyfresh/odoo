        <report
                id="action_report_fileopening"
                string="File Opening"
                model="fileopening"
                report_type="qweb-html"
                groups="base.group_user"
                file="fileopening"
                name="sf_fileopening.fileopening"
                print_report_name="'FILE OPENING - %s' % (object.name)"
        />

        <template id="sf_fileopening.fileopening">
            <t t-call="web.html_container">
                <t t-foreach="docs" t-as="o">
                    <div class="page">
                        <div class="row">
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-6">LOT:</div>
                                    <div class="col-6" t-field="o.lot"/></div>
                                <hr/>
                                <div class="row">
                                    <div class="col-6">MAWB:</div>
                                    <div class="col-6" t-field="o.mawb"/></div>
                                <div class="row">
                                    <div class="col-6">HAWB:</div>
                                    <div class="col-6" t-field="o.hawb"/></div>
                                <hr/>
                                <div class="row">
                                    <div class="col-4">
                                        <div>POL</div>
                                        <div t-field="o.pol"/></div>
                                    <div class="col-4">
                                        <div>POD</div>
                                        <div t-field="o.pod"/></div>
                                    <div class="col-4">
                                        <div>STOP</div>
                                        <div t-field="o.stop"/></div>
                                </div>
                                <hr/>
                                <div class="row">
                                    <div class="col-6">FLIGHT:</div>
                                    <div class="col-6" t-field="o.flight"/></div>
                                <hr/>
                                <div class="row">
                                    <div class="col-4">
                                        <div>ETD</div>
                                        <div t-field="o.etd"/></div>
                                    <div class="col-4">
                                        <div>ETA</div>
                                        <div t-field="o.eta"/></div>
                                    <div class="col-4">
                                        <div>DEL DATE</div>
                                        <div t-field="o.delivery_date"/></div>
                                </div>
                            </div>
                            <div class="col-6">
                                <div class="row">
                                    <div class="col-4">CONSIGNEE:</div>
                                    <div class="col-8" t-field="o.consignee"/></div>
                                <hr/>
                                <div class="row">
                                    <div class="col-4">SHIPPER:</div>
                                    <div class="col-8" t-field="o.shipper"/></div>
                                <hr/>
                                <div class="row">
                                    <div class="col-4">NOTIFY:</div>
                                    <div class="col-8" t-field="o.notify"/></div>
                            </div>
                        </div>
                        <hr/>
                        <div class="row">
                            <div class="col-2">
                                <div>INCOTERM</div>
                                <div t-field="o.incoterm"/></div>
                            <div class="col-2">
                                <div>OP AGENT</div>
                                <div t-field="o.op_agent"/></div>
                            <div class="col-2">
                                <div>SALES AGENT</div>
                                <div t-field="o.sale_agent"/></div>
                            <div class="col-2">
                                <div>IMP/EXP</div>
                                <div t-field="o.imp_exp"/></div>
                            <div class="col-2">
                                <div>PHYTO</div>
                                <div>
                                    <t t-if="o.phyto">YES</t>
                                    <t t-if="not o.phyto">NO</t>
                                </div>
                            </div>
                            <div class="col-2">
                                <div>TRUCK NB</div>
                                <div t-field="o.truck"/></div>
                        </div>
                        <hr/>
                        <div class="row">
                            <div class="col-2">
                                <div>AF</div>
                                <div>
                                    <t t-if="o.af">YES</t>
                                    <t t-if="not o.af">NO</t>
                                </div>
                            </div>
                            <div class="col-1"></div>
                            <div class="col-2">
                                <div>CURRENCY</div>
                                <div t-field="o.currency"/></div>
                            <div class="col-3">
                                <div>EXCHANGE RATE</div>
                                <div t-field="o.exchange_rate"/></div>
                            <div class="col-2"></div>
                            <div class="col-2">
                                <div>CBM</div>
                                <div t-field="o.cbm"/></div>
                        </div>
                        <hr/>
                        <div class="row">
                            <div class="col-2">
                                <div>PALETTIZED</div>
                                <div>
                                    <t t-if="o.palettized">YES</t>
                                    <t t-if="not o.palettized">NO</t>
                                </div>
                            </div>
                            <div class="col-2">
                                <div>BOXES</div>
                                <div t-field="o.box_amount"/></div>
                            <div class="col-2">
                                <div>PAL NB</div>
                                <div t-field="o.palette_amount"/></div>
                            <div class="col-2">
                                <div>GROSS WEIGHT</div>
                                <div t-field="o.weight_gross"/></div>
                            <div class="col-2">
                                <div>NET WEIGHT</div>
                                <div t-field="o.weight_net"/></div>
                            <div class="col-2">
                                <div>CHARGEABLE WEIGHT</div>
                                <div t-field="o.weight_chargeable"/></div>
                        </div>
                        <hr/>
                        <div class="row">
                            <div class="col-12 text-center">REMARKS</div>
                            <div class="col-12" t-field="o.remarks"></div>
                        </div>
                    </div>
                </t>
            </t>
        </template>
    </data>
</odoo>